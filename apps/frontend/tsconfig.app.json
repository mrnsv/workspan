{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./out-tsc/app",
    "strict": false,
    "strictNullChecks": false,
    "declaration": false,
    "moduleResolution": "node",
    "target": "ES2022",
    "module": "ES2022",
    "useDefineForClassFields": false,
    "noEmit": false,
    "types": []
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
