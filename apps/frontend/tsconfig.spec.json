{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./out-tsc/spec",
    "strict": false,
    "strictNullChecks": false,
    "declaration": false,
    "moduleResolution": "node",
    "target": "ES2020",
    "module": "ES2020",
    "noEmit": false,
    "types": [
      "jasmine"
    ]
  },
  "include": [
    "src/**/*.spec.ts",
    "src/**/*.d.ts"
  ]
}
